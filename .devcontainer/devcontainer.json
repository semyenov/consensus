{
  "name": "consensus",
  "service": "dev",

  "dockerComposeFile": "docker-compose.yml",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",

  "forwardPorts": [3000],
  "remoteUser": "node",
  "postCreateCommand": "sudo corepack enable && yarn install --frozen-lockfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "heybourn.headwind"
      ]
    }
  }
}
