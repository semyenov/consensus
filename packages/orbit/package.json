    "@libp2p/webrtc": "^4.1.2",
    "@libp2p/websockets": "^8.1.2",
    "@libp2p/webtransport": "^4.1.2",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@multiformats/dns": "^1.0.6",
